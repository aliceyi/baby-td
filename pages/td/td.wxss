/**td.wxss**/
.td-title {
  padding:5px;
  background: #FF9183;
  color: #fff;
  height: 50rpx;
  line-height: 50rpx;
  display: block;
  width: 150rpx;
  text-align: center;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
  border: 1rpx solid #FF9183;
}
.td-acount {
  padding:5px;
  width: 150rpx;
  display: inline-block;
  height: 70rpx;
  line-height: 70rpx;
  border: 1rpx solid #FFD9D6;
  border-top: 0px;
  text-align: center;
  background: #FFD9D6;
  color: #fff;
  border-bottom-right-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  font-size: 40rpx;
}
.td-unit {
  font-size: 35rpx;  
  color: #FFCBC6;
}
.down-text{
  text-align: center;
  font-size: 80rpx;
  color: #FFADA6;
}
.td-shark {
  width: 280px;
  height: 280px;
  display: inline-block; 
  text-align: center;
  margin: auto; 

}
.td-shark-text {
  text-align: center;
  position: absolute;
  left: 0px;
  right: 0px;
  margin-top: 40%;
  color: #fff;
}
.grid-cell{
  flex: 1;
  border: 1px solid #000;
}
.td-end {
  background: #FF9183;
  border: 0;
  color: #fff;
  width: 240rpx;
  box-shadow:0rpx 6rpx 6rpx 5rpx #FFCBC6 ;
  height: 74rpx;
  line-height: 74rpx;
  font-size: 34rpx;
  border-radius: 37rpx;
}