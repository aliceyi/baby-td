/**td.wxss**/
.td-title {
  padding:5px;
  background: #57bace;
  color: #fff;
  height: 50rpx;
  line-height: 50rpx;
  display: block;
  width: 150rpx;
  text-align: center;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
  border: 1rpx solid #57bace;
}
.td-acount {
  padding:5px;
  width: 150rpx;
  display: inline-block;
  height: 70rpx;
  line-height: 70rpx;
  border: 1rpx solid #ccc;
  border-top: 0px;
  text-align: center;
}
.td-unit {
  font-size: 12px;
}
.td-shark {
  background: url('../imgs/pink/shark.png')
}
.grid-cell{
  flex: 1;
  border: 1px solid #000;
}